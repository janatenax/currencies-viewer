    "less": "^2.7.3",
    "less-loader": "^4.0.5",
    "less-watch-compiler": "^1.11.0",
    "path": "^0.12.7",
    "style-loader": "^0.20.1",
    "webpack": "^3.11.0",
    "webpack-dev-server": "^2.11.1"
